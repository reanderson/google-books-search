  "bugs": {
    "url": "https://github.com/reanderson/google-books-search/issues"
  },
  "homepage": "https://github.com/reanderson/google-books-search#readme",
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6"
  },
  "dependencies": {
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.12"
  }
}
